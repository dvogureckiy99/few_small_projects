                 ;unsigned char Digit (unsigned int d, unsigned char m)
                 ;{
                 _Digit:
                 ; .FSTART _Digit
                 ;    //[]-----------------------------------------------------[]
                 ;    //| Назначение: выделение цифр из разрядов пятиразрядного |
                 ;    //| десятичного положительного числа |
                 ;    //| Входные параметры: |
                 ;    //| d - целое десятичное положительное число |
                 ;    //| m - номер разряда (от 1 до 5, слева направо) |
                 ;    //| Функция возвращает значение цифры в разряде m числа d |
                 ;    //[]-----------------------------------------------------[]
                 ;    unsigned char i = 5, a;
                 ;    while(i)
00010b 93aa      	ST   -Y,R26
00010c 931a      	ST   -Y,R17
00010d 930a      	ST   -Y,R16
                 ;	d -> Y+3
                 ;	m -> Y+2
                 ;	i -> R17
                 ;	a -> R16
00010e e015      	LDI  R17,5
                 _0xE:
00010f 3010      	CPI  R17,0
000110 f0b1      	BREQ _0x10
                 ;    {
                 ;        a = d%10; //если d < 0 то a<0
000111 81ab      	LDD  R26,Y+3
000112 81bc      	LDD  R27,Y+3+1
000113 e0ea      	LDI  R30,LOW(10)
000114 e0f0      	LDI  R31,HIGH(10)
000115 940e 029a 	CALL __MODW21U
000117 2f0e      	MOV  R16,R30
                 ;        if(i-- == m) break;
000118 931f      	PUSH R17
000119 5011      	SUBI R17,1
00011a 81ea      	LDD  R30,Y+2
00011b 91af      	POP  R26
00011c 17ea      	CP   R30,R26
00011d f049      	BREQ _0x10
                 ;        d /= 10;
00011e 81ab      	LDD  R26,Y+3
00011f 81bc      	LDD  R27,Y+3+1
000120 e0ea      	LDI  R30,LOW(10)
000121 e0f0      	LDI  R31,HIGH(10)
000122 940e 0287 	CALL __DIVW21U
000124 83eb      	STD  Y+3,R30
000125 83fc      	STD  Y+3+1,R31
                 ;    }
000126 cfe8      	RJMP _0xE
                 _0x10:
                 ;    return(a);
000127 2fe0      	MOV  R30,R16
000128 8119      	LDD  R17,Y+1
000129 8108      	LDD  R16,Y+0
00012a 9625      	ADIW R28,5
00012b 9508      	RET
                 ;}
                 ; .FEND
