                 ;unsigned char Code(unsigned char symb)
                 ;{
                 _Code:
                 ; .FSTART _Code
                 ;    //[]------------------------------------------------[]
                 ;    //| Назначение: перекодировка символов кириллицы |
                 ;    //| Входные параметры: symb – символ ASCII |
                 ;    //| Функция возвращает код отображения символа |
                 ;    //[]------------------------------------------------[]
                 ;      unsigned char TabCon[] =
                 ;    {0x41,0xA0,0x42,0xA1,0xE0,0x45,0xA3,0xA4,0xA5,0xA6,0x4B, 0xA7,0x4D,0x48,0x4F,0xA8,0x50,0x43,0x54,0xA9,0xAA,0x58,
                 ;    0xE1,0xAB,0xAC,0xE2,0xAD,0xAE,0x62,0xAF,0xB0,0xB1,0x61,
                 ;    0xB2,0xB3,0xB4,0xE3,0x65,0xB6,0xB7,0xB8,0xB9,0xBA,0xBB,
                 ;    0xBC,0xBD,0x6F,0xBE,0x70,0x63,0xBF,0x79,0x5C,0x78,0xE5,
                 ;    0xC0,0xC1,0xE6,0xC2,0xC3,0xC4,0xC5,0xC6,0xC7};//коды символов в кириллице
                 ;    return (symb >= 192 ? TabCon[symb-192]: symb);
00012c 93aa      	ST   -Y,R26
00012d 97ef      	SBIW R28,63
00012e 9721      	SBIW R28,1
00012f e480      	LDI  R24,64
000130 e0a0      	LDI  R26,LOW(0)
000131 e0b0      	LDI  R27,HIGH(0)
000132 e8ec      	LDI  R30,LOW(_0x12*2)
000133 e0f0      	LDI  R31,HIGH(_0x12*2)
000134 940e 02a7 	CALL __INITLOCB
                 ;	symb -> Y+64
                 ;	TabCon -> Y+0
                +
000136 01de     +MOVW R26 , R28
000137 5ca0     +SUBI R26 , LOW ( - 64 )
000138 4fbf     +SBCI R27 , HIGH ( - 64 )
000139 91ac     +LD R26 , X
                 	__GETB2SX 64
00013a 3ca0      	CPI  R26,LOW(0xC0)
00013b f060      	BRLO _0x13
                +
00013c 01fe     +MOVW R30 , R28
00013d 5ce0     +SUBI R30 , LOW ( - 64 )
00013e 4fff     +SBCI R31 , HIGH ( - 64 )
00013f 81e0     +LD R30 , Z
                 	__GETB1SX 64
000140 e0f0      	LDI  R31,0
000141 5ce0      	SUBI R30,LOW(192)
000142 40f0      	SBCI R31,HIGH(192)
000143 01de      	MOVW R26,R28
000144 0fae      	ADD  R26,R30
000145 1fbf      	ADC  R27,R31
000146 91ec      	LD   R30,X
000147 c004      	RJMP _0x14
                 _0x13:
                +
000148 01fe     +MOVW R30 , R28
000149 5ce0     +SUBI R30 , LOW ( - 64 )
00014a 4fff     +SBCI R31 , HIGH ( - 64 )
00014b 81e0     +LD R30 , Z
                 	__GETB1SX 64
                 _0x14:
00014c 96ef      	ADIW R28,63
00014d 9622      	ADIW R28,2
00014e 9508      	RET
                 ;}
